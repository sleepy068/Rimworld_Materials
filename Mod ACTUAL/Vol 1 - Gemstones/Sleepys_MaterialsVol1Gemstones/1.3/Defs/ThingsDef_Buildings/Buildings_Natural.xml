<Defs>

  <!-- Gemstones -->
  <ThingDef ParentName="RockBase">
    <defName>SLP_MineableDiamond</defName>
    <label>diamond</label>
    <description>Rock containing bits of diamond.</description>
    <graphicData>
      <texPath>Things/Building/Linked/RockFlecked_Atlas</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <color>(68,83,68)</color>
      <colorTwo>(30,173,25)</colorTwo>
    </graphicData>
    <statBases>
      <MaxHitPoints>1500</MaxHitPoints>
    </statBases>
    <building>
      <isResourceRock>true</isResourceRock>
      <mineableThing>SLP_Diamond</mineableThing>
      <mineableYield>40</mineableYield>
      <mineableScatterCommonality>0.04</mineableScatterCommonality>
      <mineableScatterLumpSizeRange>1~2</mineableScatterLumpSizeRange>
    </building>
  </ThingDef>
  
