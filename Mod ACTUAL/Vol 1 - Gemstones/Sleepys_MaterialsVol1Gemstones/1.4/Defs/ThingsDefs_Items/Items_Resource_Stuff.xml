<Defs>

  <!--=============== Gemstones ====================-->

  <!-- Diamond -->
  <ThingDef ParentName="ResourceBase">
    <defName>SLP_Diamond</defName>
    <label>diamond</label>
    <description>A extremely hard, colourless stone. Because of its beauty and rarity, diamond is often used for ornaments and decorations. While diamond is incredibly hard and even steel can not scratch it, a hammer will shatter it so it isn't an effective materials for weapons or armour.</description>
    <graphicData>
      <texPath>Things/Item/Resource/SLP_Gemstones_Cutstone</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
	  <color>(254,254,254)</color>
	  <drawSize>0.80</drawSize>
    </graphicData>
    <useHitPoints>false</useHitPoints>
    <healthAffectsPrice>false</healthAffectsPrice>
    <soundInteract>ChunkRock_Drop</soundInteract>
    <soundDrop>ChunkRock_Drop</soundDrop>
    <statBases>
      <MarketValue>15</MarketValue>
      <Mass>0.5</Mass>
      <StuffPower_Armor_Sharp>0.72</StuffPower_Armor_Sharp>
      <StuffPower_Armor_Blunt>0.36</StuffPower_Armor_Blunt>
      <StuffPower_Armor_Heat>0.36</StuffPower_Armor_Heat>
      <StuffPower_Insulation_Cold>3</StuffPower_Insulation_Cold>
      <StuffPower_Insulation_Heat>0</StuffPower_Insulation_Heat>
	  <SharpDamageMultiplier>0.75</SharpDamageMultiplier>
      <BluntDamageMultiplier>1.0</BluntDamageMultiplier>
    </statBases>
    <thingCategories>
      <li>SLP_Gemstones</li>
    </thingCategories>
    <deepCommonality>0.2</deepCommonality>
    <deepCountPerPortion>2</deepCountPerPortion>
    <deepLumpSizeRange>1~2</deepLumpSizeRange>
    <burnableByRecipe>false</burnableByRecipe>
    <smeltable>false</smeltable>
    <stuffProps>
      <categories>
        <li>Stony</li>
      </categories>
      <commonality>0.01</commonality>
      <color>(254,254,254)</color>
      <constructEffect>ConstructMetal</constructEffect>
      <soundImpactStuff>BulletImpact_Metal</soundImpactStuff>
      <soundMeleeHitSharp>MeleeHit_Stone</soundMeleeHitSharp>
      <soundMeleeHitBlunt>MeleeHit_Stone</soundMeleeHitBlunt>
      <statOffsets>
        <Beauty>30</Beauty>
      </statOffsets>
      <statFactors>
        <MaxHitPoints>0.6</MaxHitPoints>
        <WorkToMake>1.4</WorkToMake>
        <WorkToBuild>5.0</WorkToBuild>
        <Beauty>2.5</Beauty>
        <Flammability>0</Flammability>
        <MeleeWeapon_CooldownMultiplier>1.0</MeleeWeapon_CooldownMultiplier>
      </statFactors>
    </stuffProps>
	<tradeTags>
      <li>ExoticMisc</li>
    </tradeTags>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <allowedArchonexusCount>20</allowedArchonexusCount>
  </ThingDef>
  
