<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
  <RecipeDef>
    <defName>SLPMake_TitaniumGold</defName>
    <label>make titanium gold</label>
    <description>Make titanium gold alloy by smelting gold and titanium together.</description>
    <jobString>Making alloy (titanium gold).</jobString>
	<recipeUsers>
        <li>ElectricSmelter</li>
    </recipeUsers>
    <workAmount>1200</workAmount>
    <workSpeedStat>SmeltingSpeed</workSpeedStat>
    <effectWorking>Smelt</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>SLP_Titanium</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>Gold</li>
          </thingDefs>
        </filter>
        <count>0.1</count>
      </li>
    </ingredients>
    <products>
      <SLP_TitaniumGold>1</SLP_TitaniumGold>
    </products>
    <fixedIngredientFilter>
      <thingDefs>
        <li>SLP_Titanium</li>
		<li>Gold</li>
      </thingDefs>
    </fixedIngredientFilter>
  </RecipeDef>
  
